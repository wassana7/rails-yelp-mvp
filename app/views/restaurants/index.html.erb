<h1>Restaurants</h1>

<ul>
  <% @restaurants.each do |restaurant| %>
  <li> <%= link_to restaurant.name, restaurant_path(restaurant) %> <%= restaurant.address %> <%= restaurant.phone_number %> <%= restaurant.category %></li>
  <% end %>
</ul>

<%= link_to('Add a restaurant', new_restaurant_path(@restaurant))  %>
